  "author": "Raphael Thelen <raphael.thelen@rwth-aachen.de>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npx @webwriter/build"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.15.0",
    "@webwriter/lit": "^2.0.0",
    "lit": "^3.1.3"
  },
  "devDependencies": {
    "typescript": ">=4.8.4 <5.0.0-0",
    "vite": ">=3.1.6 <4.0.0-0"
  }
